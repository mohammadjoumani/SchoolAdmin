<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/gray"    tools:context=".view.fargment.StudentFragment">    <ImageView        android:id="@+id/imgFragmentStudentEmpty"        android:layout_width="220dp"        android:layout_height="220dp"        android:layout_centerInParent="true"        android:src="@drawable/ic_empty"        android:visibility="gone" />    <ImageView        android:id="@+id/imgFragmentStudent404"        android:layout_width="220dp"        android:layout_height="220dp"        android:layout_centerInParent="true"        android:src="@drawable/ic_empty"        android:visibility="gone" />    <ProgressBar        android:id="@+id/progressBarFragmentStudentLoad"        style="?android:attr/progressBarStyle"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerInParent="true"        android:visibility="gone" />    <RelativeLayout        android:id="@+id/toolBarFragmentHome"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="15dp">        <TextView            android:id="@+id/txtFragmentHomeNameFragment"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:fontFamily="@font/roboto_medium"            android:text="Student"            android:textColor="@color/black"            android:textSize="26sp" />        <ImageView            android:layout_width="35dp"            android:layout_height="35dp"            android:src="@drawable/ic_user"            android:layout_alignParentEnd="true"            android:layout_centerVertical="true"/>    </RelativeLayout>    <RelativeLayout        android:id="@+id/linearSpinnerClass"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="5dp"        android:layout_marginEnd="15dp"        android:layout_marginStart="15dp"        android:layout_below="@+id/toolBarFragmentHome"        android:orientation="horizontal">        <ImageButton            android:id="@+id/btnAddClass"            android:layout_width="35sp"            android:layout_height="35sp"            android:background="@drawable/ic_add"            android:layout_centerVertical="true" />        <Spinner            android:id="@+id/spinnerClass"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_toEndOf="@+id/btnAddClass"           android:background="@drawable/bg_spinner" />        <ImageView            android:layout_width="15dp"            android:layout_height="15dp"            android:layout_centerVertical="true"            android:layout_marginEnd="10dp"            android:layout_alignParentEnd="true"           android:src="@drawable/ic_below"/>    </RelativeLayout>    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/recyclerFragmentStudent"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/linearSpinnerClass"        android:layout_marginStart="10dp"        android:layout_marginTop="5dp"        android:layout_marginEnd="10dp"        android:orientation="vertical"        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"        tools:listitem="@layout/item_recycler_student" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/floatingActionFragmentStudentAddStudenet"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentEnd="true"        android:layout_alignParentBottom="true"        android:layout_marginEnd="15dp"        android:layout_marginBottom="5dp"        android:clickable="true"        android:elevation="8dp"        app:backgroundTint="@color/purple_200"        app:srcCompat="@drawable/ic_add_estate" /></RelativeLayout>