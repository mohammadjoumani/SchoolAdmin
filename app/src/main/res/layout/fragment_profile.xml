<?xml version="1.0" encoding="utf-8"?><RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".view.fargment.ProfileFragment"    android:background="@color/gray">    <RelativeLayout        android:id="@+id/toolBarFragmentHome"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="15dp">        <TextView            android:id="@+id/txtFragmentHomeNameFragment"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:fontFamily="@font/roboto_medium"            android:text="Profile"            android:textColor="@color/black"            android:textSize="26sp" />        <ImageView            android:layout_width="35dp"            android:layout_height="35dp"            android:src="@drawable/ic_user"            android:layout_alignParentEnd="true"            android:layout_centerVertical="true"/>    </RelativeLayout>    <ImageView        android:id="@+id/imgProfileFragmentProfile"        android:layout_width="150dp"        android:layout_height="150dp"        android:layout_below="@+id/toolBarFragmentHome"        android:layout_centerHorizontal="true"        android:layout_marginTop="20dp"        android:scaleType="centerCrop"        android:src="@drawable/ic_admin" />    <RelativeLayout        android:id="@+id/rel1"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/imgProfileFragmentProfile"        android:layout_marginTop="30dp">        <ImageView            android:id="@+id/imgIconAccount"            android:layout_width="35dp"            android:layout_height="35dp"            android:layout_marginStart="10dp"            android:layout_marginEnd="20dp"            android:src="@drawable/ic_account" />        <TextView            android:id="@+id/txtNameAccount"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_toEndOf="@+id/imgIconAccount"            android:fontFamily="@font/roboto_regular"            android:text="@string/name"            android:textColor="@color/colorPrimaryDark"            android:textSize="14sp" />        <TextView            android:id="@+id/txtNameAccountFragmentProfile"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@+id/txtNameAccount"            android:layout_toEndOf="@+id/imgIconAccount"            android:fontFamily="@font/roboto_regular"            android:text="Mohoammad"            android:textColor="@color/black"            android:textSize="18sp" />        <View            android:layout_width="match_parent"            android:layout_height="2dp"            android:layout_below="@id/txtNameAccountFragmentProfile"            android:layout_marginStart="30dp"            android:layout_marginTop="25dp"            android:layout_marginEnd="25dp"            android:background="@color/colorPrimaryDark" />    </RelativeLayout>    <RelativeLayout        android:id="@+id/rel2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/rel1"        android:layout_marginTop="25dp">        <ImageView            android:id="@+id/imgIconPhone"            android:layout_width="35dp"            android:layout_height="35dp"            android:layout_marginStart="10dp"            android:layout_marginEnd="20dp"            android:src="@drawable/selectot_password" />        <TextView            android:id="@+id/txtNumberAccount"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_toEndOf="@+id/imgIconPhone"            android:fontFamily="@font/roboto_regular"            android:text="@string/hint_password"            android:textColor="@color/colorPrimaryDark"            android:textSize="14sp" />        <TextView            android:id="@+id/txtPasswordAccountFragmentProfile"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@+id/txtNumberAccount"            android:layout_toEndOf="@+id/imgIconPhone"            android:fontFamily="@font/roboto_regular"            android:text="0940741908"            android:textColor="@color/black"            android:textSize="18sp" />        <View            android:layout_width="match_parent"            android:layout_height="2dp"            android:layout_below="@id/txtPasswordAccountFragmentProfile"            android:layout_marginStart="30dp"            android:layout_marginTop="25dp"            android:layout_marginEnd="25dp"            android:background="@color/colorPrimaryDark" />    </RelativeLayout></RelativeLayout>